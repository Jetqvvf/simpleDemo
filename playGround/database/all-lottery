<script type="text/javascript"> 
function tipfight(tid,pid,fid){
	var state=1,authorid = $("#tipcolor"+pid).attr("mid"),token=$("#tipcolor"+pid).attr("token");
	$.ajax({
		type: "POST",
		dataType: "json",
		url: "/ajax/lights.ajax.php?" + Math.random(),
		data: {tid:tid, pid:pid, state:state, authorid:authorid, fid:fid,token:token},
		success: function(data) {
			$("b.loading").hide();
			if(data.code != 1){
				if(data.code == -1){
					show_login();
					ga("bbs-read-highlights-showLogin");
				}else if(data.code == -2){
					show_message("<div class='tips_pop'>出错了，请刷新后再试下</div>");
				}else if(data.code == -3){
					show_message("<div class='tips_pop'>出错了，请刷新后再试下</div>");
				}else if(data.code == -7){
					show_message("<div class='tips_pop'>你已经点亮过这个回帖了</div>");
				}else if(data.code == -8){
					show_message("<div class='tips_pop'>你已经点灭过这个回帖了</div>");
				}else if(data.code == -9){
					show_message("<div class='tips_pop'>您由于违规使用已被取消使用该功能，请前往版务区咨询。</div>");
				}

				return false;
			}else{
				$("#tip"+pid).text(data.num);
				if(state==1){
					$("#tipcolor"+pid).attr("class","red");
				}else{
					$("#fightcolor"+pid).attr("class","f666");
				}
				ga("bbs-c-highlights-click");
			}
		}
	});
}
window.onload=function(){
	var mw = 550;if(screen.width > 1152){mw = 760;}$('table.case img').each(function(){
		if($(this).width() > mw){
		$(this).width(mw).attr('title','点击查看原图').addClass('lightbox').click(function(){window.open($(this).attr('src'));});}});}
</script> 


					voiceRemarks.init({elem:"voiceRemarks",box:"voiceRemarks-box",up:"btn-up",down:"btn-down",height:"347"});
					function scroll(n){
						temp=n;
						var News = document.getElementById("data1");
						News.scrollTop=News.scrollTop+temp;
						if (temp == 0 || News.scrollTop == 0) return;
						setTimeout("scroll(temp)",25);
					}

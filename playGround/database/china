		</div>
        <script type="text/javascript">
            //初始化加载当前轮
                        changeTurn( 23, '#csl_boxcore',128,30);
                                    function changeTurn( turn, divId, leagueId, maxTurn){
                if(turn<=0 || turn>maxTurn)
                {
                    return false;
                };
                $.post("https://soccer.hupu.com/china/cslBoxscore.php",{turn:turn, leagueId:leagueId, divId:divId},function(data){
                    $(divId).html(data);
                    if(turn-1<=0){
                        $(divId +" .btn-prev").addClass("btn-noPrev");
                    };
                    if(turn>maxTurn-1){
                        $(divId + " .btn-next").addClass("btn-noNext");
                    };
                });
            }
        </script>

      function tipfight(tid,pid,fid){
        var state=1,authorid = $("#tipcolor"+pid).attr("mid"),token="4306b06d0c";
        $.ajax({
          type: "POST",
          dataType: "json",
          url: "/ajax/lights.ajax.php?" + Math.random(),
          data: {tid:tid, pid:pid, state:state, authorid:authorid, fid:fid,token:token},
          success: function(data) {
              $("b.loading").hide();
              if(data.code != 1){
                  if(data.code == -1){
                      show_login();
                  }else{
                      show_message("<div class='tips_pop'>"+data.msg+"</div>");
                  }

                  return false;
              }else{
                  $("#tip"+pid).text(data.num);
                  if(state==1){
                      $("#tipcolor"+pid).attr("class","red");
                  }else{
                      $("#fightcolor"+pid).attr("class","f666");
                  }
                  ga("bbs-c-highlights-click");
              }
          }
        });
      }

<script>
function stick_search(){
	var search_keyword = $("#search_keywordnew").val();
	location.href='/search/board-'+search_keyword+'-1';
}

var isIE = navigator.userAgent.indexOf("MSIE")>0;
//创建事件的通用函数
var EventUtil = function(){};
EventUtil.winWidth   = 0;
EventUtil.winHeight = 0;
EventUtil.addEventHandler = function(obj,EventType,Handler)
{
	//如果是FF
	if(obj.addEventListener)
	{
	   obj.addEventListener(EventType,Handler,false);
	}
	//如果是IE
	else if(obj.attachEvent)
	{
	   obj.attachEvent('on'+EventType,Handler);
	}
	else
	{
	   obj['on'+EventType] = Handler;
	}
}

/**
* @deprecated 获取浏览器 Event 事件
* @return evt
*/
EventUtil.getEvent = function (evt)
{
	return (evt ? evt : (window.event ? window.event : EventUtil.getEvent.caller.arguments[0]));
}
$("#search_keywordnew").keydown( function() { 
	if (window.attachEvent)
    {
		if(event.keyCode==13){
			stick_search(); 
		}
    }else{
		var eventnew = EventUtil.getEvent(eventnew);
		if(eventnew.keyCode==13){
			stick_search();
		}
	}	
}); 
</script>
</div>
